{
  "name": "@parcnet-js/app-connector-react",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./src/index.ts",
        "require": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "tsup 'src/**/*@(ts|tsx)' --format cjs,esm --clean --sourcemap",
    "prepublish": "tsup 'src/**/*@(ts|tsx)' --format cjs,esm --clean --sourcemap --dts",
    "test": "vitest run --typecheck"
  },
  "files": ["dist", "LICENSE"],
  "dependencies": {
    "@lit/react": "^1.0.6",
    "@parcnet-js/app-connector": "workspace:*"
  },
  "devDependencies": {
    "@parcnet-js/eslint-config": "workspace:*",
    "@parcnet-js/typescript-config": "workspace:*",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.11",
    "react": "^18.3.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "import": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "require": "./dist/index.cjs"
        }
      }
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": "^18.3.1"
  }
}
